{
    "name": "bangnokia/psycho",
    "type": "library",
    "description": "The psycho library",
    "license": "MIT",
    "require": {
        "php": "7.x|^8.0",
        "psy/psysh": "^0.10",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "8.x|^9.5",
        "symfony/process": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "BangNokia\\Psycho\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BangNokia\\Psycho\\Tests\\": "tests/"
        }
    },
    "scripts":{
        "build": "composer install --no-dev -o && ./bin/box.phar compile"
    }
}
